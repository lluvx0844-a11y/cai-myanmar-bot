{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/index",
      "dest": "api/index.py"
    },
    {
      "src": "/index.html",
      "dest": "public/index.html"
    }
  ]
}
